{
  "env": {
    "test": {
      "plugins": [
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "Redux": "./redux"
          }
        }],
        ["transform-class-properties"],
        ["transform-es2015-arrow-functions"],
        ["transform-object-rest-spread"],
        ["transform-decorators-legacy"],
        [ "transform-es2015-modules-commonjs", { "loose": true } ],
        [ "transform-react-jsx", { "pragma": "h" } ]
      ]
    }
  }
}